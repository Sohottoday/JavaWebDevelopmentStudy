 package loopexample;

import java.util.Scanner;

public class Lab {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.print("홀수를 입력해주세요 : ");
		int num = sc.nextInt();
		char star = '*';
		System.out.println();
		
		for(int n=1; n<=num; n++) {
			System.out.println((star * n));
			if((n % 2) == 0) {
				continue;
			}
		}
		
		
		
		
		sc.close();
		
	}

}
