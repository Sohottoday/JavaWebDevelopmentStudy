package cooperation;

public class MyDate {

	private int day;
	private int month;
	private int year;
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
	public MyDate(int day, int month, int year) {
		
		switch(month) {
			case 1: case 3:  case 5:  case 7:  case 9:  case 11:
				if(day < 31 && day > 0) {
					isVaild();
				}
		}
	}
	
	public boolean isValid() {
		
		
		return true;
	}
	
}
