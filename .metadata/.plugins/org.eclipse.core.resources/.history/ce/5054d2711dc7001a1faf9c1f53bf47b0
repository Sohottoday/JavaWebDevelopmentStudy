package loopexample;

public class NestedLoop {

	public static void main(String[] args) {

		int dan;
		int count;
				
		for(dan=2; dan<=9; dan++) {
			for(count=2; count<=9; count++) {
				System.out.print(dan + " x " + count + " = " + (dan*count) + "	");
			}
			System.out.println();
		}
		
		dan = 2;
		count = 1;
		
		while(dan <= 9) {
			while(count <= 9) {
				System.out.print(dan + " x " + count + " = " + (dan*count) + "	");
				count++;
			}
			System.out.println();
		}
		
	}

}
