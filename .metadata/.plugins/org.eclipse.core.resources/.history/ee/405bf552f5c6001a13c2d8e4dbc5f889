package operator;

public class OperatorEx2 {

	public static void main(String[] args) {

		int score = 100;
		System.out.println(++score);
		System.out.println(score++);
		System.out.println(score);
		
		int num1 = 10;
		int i = 2;
		
		boolean value = ((num1 = num1 + 10) < 10) && ((i = i+2) < 10);
		System.out.println(value);
		
	}

}
