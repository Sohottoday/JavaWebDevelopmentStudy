package array;

public class ArrayTest {

	public static void main(String[] args) {

		int[] arraytest = new int[] {1, 2, 3};
		//int[] arraytest = {1, 2, 3};		단, 선언과 동시에 값을 부여하는 경우에만 사용한다.
		
		int[] arr = new int[10];
		for(int i=0; i<arr.length; i++) {
			System.out.print(arr[i] + "\t");
		}
		
	}

}
