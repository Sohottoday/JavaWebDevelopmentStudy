package cooperation;

public class MyDate {

	private int day;
	private int month;
	private int year;
	private boolean isVaild = true;
	
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		switch(month) {
		case 1: case3: case5: case7: case9: case11:
			isVaild = false;
		}
	}
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
	public MyDate(int day, int month, int year) {
		setDay(day);
		setMonth(month);
		setYear(year);

	}
	
	public boolean isValid() {
		
		
		return true;
	}
	
}
