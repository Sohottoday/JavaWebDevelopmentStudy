	public static void main(String[] args) {

		try(FileOutputStream fos = new FileOutputStream("data.txt");
				DataOutputStream dos = new DataOutputStream(fos)
				){
			dos.writeByte(100);
			dos.writeChar('A');
			dos.writeUTF("æ»≥Á«œººø‰");
			
		}
	}

}
